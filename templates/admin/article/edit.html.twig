{% extends "base.html.twig" %}
{% block body %}
	<h1>edition article</h1>
	<div class=" flex w-96 mx-auto p-5 shadow-lg rounded-lg">
		{{ form_start(form) }}
		{{form_row(form.title)}}
		{{form_row(form.content)}}
		{{form_row(form.image)}}
		{{form_row(form.created_at)}}

		<button class="text-green  text-xl border-green border-2 p-3 rounded-lg focus:outline-none my-5" type="submit">Enregistrer</button>

		{{form_end(form)}}</div>
{% endblock %}
